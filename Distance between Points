#Basic Problem solving with classes
class Point():
    def __init__(self, x1, y1):
        self.x1=x1
        self.y1=y1
        
    def distance(self,x2,y2):
        self.x2=x2
        self.y2=y2
        d=(((x2-self.x1)**2-(y2-self.y1)**2)**(1/2))
        print (d)

p1=Point(3,8)
p1.distance(7,5)
